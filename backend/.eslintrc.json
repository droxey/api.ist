{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "plugins": ["node"],
  "extends": ["airbnb", "plugin:node/recommended", "prettier"],
  "rules": {
    "linebreak-style": "off",
    "no-console": "off"
  }
}
